/* pages/topup/topup.wxss */
.hd_icon {
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}
.img45 {
  width: 45rpx;
  height: 45rpx;
}
.czflex {
  display: flex;
  align-items: flex-end;
}
.fw800{
  font-weight: 800;
}
.czflex input{
  line-height: 1.2rem;
  height: 70rpx;
  font-size: 60rpx;
}
radio .wx-radio-input{
  width: 18px;
  height: 18px;
}
.weui-uploader__input-box {
  width: 180rpx;
  height: 180rpx;
  margin-top: 12rpx;
}

.imgup {
  width: 180rpx;
  height: 180rpx;
}
.weui-uploader__file {
  position: relative;
  padding-top: 12rpx;
}

.weui-uploader__file .dele-btn {
  color: #fff;
  position: absolute;
  top: 0rpx;
  right: -5rpx;
  height: 30rpx;
  width: 30rpx;
  border-radius: 50%;
  line-height: 26rpx;
  text-align: center;
  background-size: 30rpx 30rpx;
  background-image: url("data:image/png;base64,");
}

.weui-uploader__input-box:before {
  	width: 4rpx;
	height: 20rpx;
  background-color: #333333;
  top: 40%;
}

.weui-uploader__input-box:after {
 	width: 20rpx;
	height: 4rpx;
  background-color: #333333;
  top: 40%;
}
.weui-uploader__input-box {
  border-style: dashed;
}
.weui-uploader__input-box view {
  position:absolute;
  top:50%;
  width:100%;

}
.btn-area{
  position: absolute;
  bottom:0px;
  width: 690rpx;
}
.weui-uploader{
  padding-left:60rpx;
  padding-bottom: 120rpx;
}