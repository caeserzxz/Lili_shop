/* pages/express/express.wxss */
.btn1 {
  width:100rpx;
height:46rpx;
line-height: 46rpx;
text-align: center;
border:1px solid rgba(246,82,54,1);
color:rgba(246,82,54,1);
font-size: 28rpx
}
.wl_img1 {
  width: 47rpx;
  height:37rpx;
  vertical-align: sub;
  margin-right: 10rpx;
}
.b-item {
  align-items: flex-start;
  padding:0px 40rpx;
}
.b-item:first-of-type .b-item_bd:after {
  content: "";
  position: absolute;
  width:34rpx;
  height:34rpx;
    top: 0px;
  left: -17rpx;
  background-size: 34rpx 34rpx;
  background-image: url('data:image/png;base64,');
}
.b-item:last-of-type .b-item_bd{
  border:none;
}
.b-item_hd {
  margin-right: 30rpx;
  width: 130rpx;
}
.b-item_bd {
  padding-left: 30rpx;
  border-left:2rpx dashed rgba(238,238,238,1);
  position: relative;
  padding-bottom: 40rpx;
}
.b-item_bd:before {
  content: "";
  position: absolute;
  left:0rpx;
  top: 8rpx;
  width:12rpx;
  height:12rpx;
  background:rgba(238,238,238,1);
  border-radius:50%;
  transform: translateX(-51%);
}
.b-items {
  padding: 90rpx 0rpx;
}
.border-before::before{
  display: none;
}
.bg-fff{
  background-color: #fff;
}