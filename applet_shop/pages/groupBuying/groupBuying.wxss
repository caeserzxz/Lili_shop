.progress_box {
  display: flex;
  align-items: center;
}

.ml20 {
  margin-left: 20rpx;
}

.hd_img_box {
  position: relative;
  margin-right: 20rpx;
  width: 160rpx;
  height: 160rpx;
}

.hd_img_box .mask {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.6);
  color: #fff;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}

.hd_img_box .mask::before {
  content: "";
  position: absolute;
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  border: 4rpx solid #fff;
  opacity: 0;
}

.cell_title {
  padding-top: 35rpx;
  padding-bottom:0rpx;
  font-size: 32rpx;
  font-weight: 500;
  padding-left: 30rpx;
}
.badge{
  color: #fff;
  background-color: #F65236;
  font-size: 22rpx;
  line-height: 1em;
  padding: 0 10rpx;
  position: relative;
   top: -3rpx;
   left: 10rpx;
}
.group_cells .weui-cell::before{
  display: none;
}
.group_cells .avatar{
  width: 70rpx;
