.hd_img {
    width: 50rpx;
    height: 50rpx;
    margin-right: 10rpx;
}

.withdraw_list {
    margin: 0 20rpx 20rpx;
    overflow: hidden;
}

.swipeout_btns {
    position: absolute;
    width: 120rpx;
    background-color: #333;
    left: 100%;
    top: 0rpx;
    bottom: 0rpx;
    display: flex;
    flex-direction: column;
    z-index: 999;
}

.cell_swiped {
    position: relative;
}

.swipeout_btns .btn1 {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
}

.swipeout_btns image {
    width: 46rpx;
    height: 46rpx;
}

.page2 {
    padding-bottom: 140rpx;
}

.btn_box {
    position: fixed;
    z-index: 999;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: #f5f5f5;
}

.mask {
    background-color: rgba(0, 0, 0, 0.6);
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    z-index: 9998;
}

.select_panel {
    transition: all 0.3s;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 9999;
    transform: translateY(100%);
}

.panel_hd_img {
    width: 60rpx;
    height: 60rpx;
    margin-right: 20rpx;
}

.icon_ok {
    display: inline-block;
    width: 10rpx;
    height: 20rpx;
    border: 4rpx solid #f65236;
    transform: rotate(45deg);
    border-top: none;
    border-left: none;
    opacity: 0;
}

.active .icon_ok {
    opacity: 1;
}

.close_x {
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 48rpx;
    text-align: center;
    color: #333;
}

.panel_show {
    transform: translateY(0);
}

.no-data{
    line-height: 200rpx;
    font-size: 28rpx;
    color: #555;
    position: fixed;
    top: 40%;
    margin-top: -100rpx;
    text-align: center;
    left: 50%;
    margin-left: -100rpx;
}

.no-data image{
    width: 200rpx;
    height: 200rpx;
}