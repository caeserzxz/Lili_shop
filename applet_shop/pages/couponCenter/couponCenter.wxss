.big_coupon {
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  margin-bottom: 20rpx;
  height: 200rpx;
  position: relative;
}

.big_coupon:last-child {
  margin-bottom: 0rpx;
}

.big_coupon .hd {
  height: 200rpx;
  position: relative;
  background-color: #fff;
  color: #333;
  width: 260rpx;
  text-align: center;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.big_coupon .hd text {
  font-size: 60rpx;
  position: relative;
  top: -10rpx;
}

.big_coupon .bd {
  height: 200rpx;
  flex: 1;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-left: 40rpx;
  border-right: 2rpx dashed rgba(238, 227, 225, 1);
}

.big_coupon .bd::before {
  content: "";
  position: absolute;
  width: 22rpx;
  height: 22rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 50%;
  right: 0;
  top: 0;
  transform: translate(50%, -50%);
  z-index: 9;
}

.big_coupon .bd::after {
  content: "";
  position: absolute;
  width: 22rpx;
  height: 22rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 50%;
  right: 0;
  bottom: 0;
  transform: translate(50%, 50%);
  z-index: 9;
}

.big_coupon .ljlq_btn {
  border: 1px solid #55b5ff;
  border-radius: 60rpx;
  padding: 10rpx 20rpx;
  color: #55b5ff;
  font-size: 24rpx;
}

.coupon_list_box {
  padding: 20rpx;
  overflow: auto;
}

page {
  background: rgba(245, 245, 245, 1);
}

.rtok {
  display: none;
  position: absolute;
  right: 0;
  top: 0;
  z-index: 99;
  width: 70rpx;
  height: 70rpx;
}

.rtok image {
  width: 100%;
  height: 100%;
}

.active .rtok {
  display: block;
}

.swiper-tab {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  z-index: 999;
  width: 100%;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-around;
  font-size: 32rpx;
  background-color: #fff;
  font-weight: 600;
}

.swiper-tab-item {
  flex: 1;
  color: #999;
}

.swiper-tab .active {
  color: #333;
  font-size: 32rpx;
  font-weight: bold;
  position: relative;
}

.swiper-tab .active::after {
  content: '';
  display: inline-block;
  position: absolute;
  bottom: 0;
  width: 24rpx;
  height: 6rpx;
  background-color: #f65236;
  left: 50%;
}

.sbtn {
  width: 124rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border: 1px solid #f65236;
  color: #f65236;
  margin-top: 10rpx;
}

.big_coupon .hd_price{
  line-height: 1em;
}
.big_coupon .hd_price .rmb{
  font-size: 28rpx;
