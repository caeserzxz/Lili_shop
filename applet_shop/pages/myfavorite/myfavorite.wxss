.page2 {
  padding-top: 130rpx;
  padding-bottom: 120rpx;
}

.top_box {
  margin-top: 0px;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 999;
  box-shadow: 0px 10rpx 20rpx 0rpx rgba(0, 0, 0, 0.04);
}

.bottom_box {
  position: fixed;
  bottom: 1px;
  left: 0;
  right: 0;
  z-index: 999;
}

.bottom_box .weui-cell {
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.qx_btn {
  width: 200rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  background: rgba(246, 82, 54, 1);
  text-align: center;
  color: #fff;
  margin-right: -18rpx;
}

.bottom_box .price_Y {
  font-size: 28rpx;
  color: #999;
}

.bottom_box .price {
  font-size: 40rpx;
  color: #333;
}

.cart_panel .hd_img {
  width: 140rpx;
  height: 140rpx;
  margin-right: 20rpx;
}

.border-none::after, .border-none::before {
  display: none !important;
}

.cart_panel .weui-cell {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.cart_panel {
  padding: 30rpx;
}

.cart_panel .weui-panel__hd {
  padding-bottom: 10rpx;
  padding-top: 10rpx;
}

.card_lilst .hd_img {
  width: 140rpx;
  height: 140rpx;
  margin-right: 20rpx;
}

.card_lilst .price {
  font-size: 38rpx;
}

.card_lilst .sprice {
  font-size: 28rpx;
  text-decoration: line-through;
  color: #999;
  margin-left: 20rpx;
}

.card_list .title {
  font-size: 28rpx;
}

.card_lilst .checkbox {
  margin-right: 20rpx;
}

.count {
  display: inline-flex;
  height: 44rpx;
  border-radius: 3px;
  align-items: center;
}

.count_btn {
  width: 44rpx;
  height: 44rpx;
  position: relative;
  background-repeat: no-repeat;
  background-size: 24rpx 24rpx;
  background-position: center center;
}
.count_btn image{
  width: 100%;
  height: 100%;
}

.count_number {
  width: 64rpx;
  height: 44rpx;
  line-height: 44rpx;
  font-size: 28rpx;
  color: #333;
  outline: none;
  border: none;
  text-align: center;
  pointer-events: none;
  border-radius: 0;
  -webkit-appearance: none;
  font-weight: 600;
}

.btn_group{
  width: 400rpx;
  display: flex;
  margin-right: -18rpx;
}
.btn_group > view{
  flex: 1;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #333;
  text-align: center;
  color: #fff;
}
.btn_group .btn2{
  background-color: #F65236;
}

.nodata{
  padding-top: 200rpx;
  background-color: #fff;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  text-align: center;
  z-index: 99999;
}
.nodata image{
  width: 750rpx;
}
.swipeout_btns{
  position: absolute;
  width: 120rpx;
  background-color: #333333;
  left: 100%;
  top: 20rpx;
  bottom: 20rpx;
  display: flex;
  flex-direction: column;
}
.cell_swiped{
  position: relative;
}
.swipeout_btns .btn1{
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}
.swipeout_btns image{
  width: 46rpx;
  height:46rpx;
}
.money{
  line-height: 40rpx;
}
.cell_swiped .weui-cell__bd{
  height: 140rpx;
}