.weui-cell {
    padding-top: 40rpx;
    padding-bottom: 40rpx;
}

.badge {
    line-height: 1em;
    height: 30px;
    background: rgba(246, 82, 54, 1);
    color: #fff;
    font-size: 22rpx;
    padding: 0 10rpx;
    margin-left: 10rpx;
}

.default {
    background-repeat: no-repeat;
    background-position: left bottom;
    background-size: 100% 4rpx;
    background-image: url(data:image/png;base64,);
}

.page2 {
    padding-bottom: 140rpx;
}

.btn_box {
    position: fixed;
    z-index: 999;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: #f5f5f5;
}

.swipeout_btns {
    position: absolute;
    width: 120rpx;
    background-color: #333;
    left: 100%;
    top: 0rpx;
    bottom: 0rpx;
    display: flex;
    flex-direction: column;
    z-index: 999;
}

.cell_swiped {
    position: relative;
}

.swipeout_btns .btn1 {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
}

.swipeout_btns image {
    width: 46rpx;
    height: 46rpx;
}

.delete {
    float: right;
    color: #666;
    font-size: 24rpx;
}

.no-data {
    line-height: 200rpx;
    font-size: 28rpx;
    color: #555;
    position: fixed;
    top: 40%;
    margin-top: -100rpx;
    text-align: center;
    left: 50%;
    margin-left: -100rpx;
}

.no-data image {
    width: 200rpx;
    height: 200rpx;
}
