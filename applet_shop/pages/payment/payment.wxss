/* pages/payment/payment.wxss */
.default{
  margin-top: 0;
  background-repeat: no-repeat;
  background-position: left bottom;
  background-size: 100% 4rpx;
  background-image: url(data:image/png;base64,)
}
.title{
  padding: 20rpx 30rpx 0;
}
.goodsinfo::before{
  display: none;
}
.goodsinfo .goodsimg{
  width: 160rpx;
  height: 160rpx;
  margin-right: 20rpx;
}
.goodsinfo .weui-cell__bd{
  height: 160rpx;
  display: flex;
  flex-direction: column;
}
.goodsinfo .weui-cell_bd .name{
      overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.goodsinfo .weui-cell_bd .type{
  display: block;
}
.monBox {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: auto;
}
.monBox .left{
  display: flex;
  align-items: baseline;
  margin-top: 16rpx;
}
.monBox .left>text{
  text-decoration:line-through;
  margin-left: 10rpx;
}
.monBox .right{
  display: flex;
  align-items: center;
}
.monBox .right image{
  width: 44rpx;
  height: 44rpx;
}
.monBox .right input{
  width: 80rpx;
  text-align: center;
}
.wordnum{
  margin-left: 20rpx;
}
.list{
  padding: 20rpx 0;
  margin-bottom: 110rpx;
}
.list .weui-cell::before{
  display: none;
}
.list .weui-cell{
  padding: 24rpx 30rpx;
}
.ticket{
  display: flex;
  align-items: center;
}
.list .tips{
  color: #fff;
  margin-right: 10rpx;
  padding: 0 10rpx;
  height:30rpx;
  background:rgba(246,82,54,1);
  display: flex;
  align-items: center;
  justify-items: center;
  font-size: 22rpx;
}
.bottomBtn{
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 16rpx 0 30rpx;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  width: 100vw;
  box-sizing: border-box;
}
.bottomBtn .left{
  display: flex;
  align-items: baseline;
}
.bottomBtn .left view{
  margin-left: 16rpx;
}
.bottomBtn .right{
  width:200rpx;
height:80rpx;
display: flex;
align-items: center;
justify-content: center;
background:rgba(246,82,54,1);
}
.no-address{
  color: #999;
  font-size: 32rpx;
}