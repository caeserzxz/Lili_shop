.mt5 {
  margin-top: 5rpx;
}

.sprice {
  text-decoration: line-through;
  margin-left: 20rpx;
}

.gobtn {
  display: inline-block;
  text-align: center;
  width: 100rpx;
  height: 46rpx;
  line-height: 46rpx;
  color: #f65236;
  border: 1px solid #f65236;
}

.gobtn_gray {
  background: rgba(238, 238, 238, 1);
}

.seckill_cells {
  padding-bottom: 20rpx;
}

.seckill_cells .hd_img {
  width: 100%;
  height: 100%;
}

.seckill_cells .title {
  line-height: 1.4em;
}

.seckill_cells .weui-cell {
  padding: 30rpx;
}

.progress_box {
  display: flex;
  align-items: center;
}

.ml20 {
  margin-left: 20rpx;
}

.hd_img_box {
  position: relative;
  margin-right: 20rpx;
  width: 160rpx;
  height: 160rpx;
}

.hd_img_box .mask {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.6);
  color: #fff;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}

.hd_img_box .mask::before {
  content: "";
  position: absolute;
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  border: 4rpx solid #fff;
  opacity: 0;
}

.cell_title {
  padding-top: 35rpx;
  padding-bottom: 0rpx;
  font-size: 32rpx;
  font-weight: 500;
  padding-left: 30rpx;
}

.badge {
  color: #fff;
  background-color: #f65236;
  font-size: 22rpx;
  line-height: 1em;
  padding: 0 10rpx;
  position: relative;
  top: -3rpx;
  left: 10rpx;
}
.group_cells{
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.group_cells .weui-cell::before {
  display: none;
}

.group_cells .avatar {
  width: 70rpx;
  height: 70rpx;
  margin-right: 20rpx;
}

.pb0 {
  padding-bottom: 0 !important;
}

.num {
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  background: rgba(192, 192, 192, 1);
  border: 1px solid rgba(238, 238, 238, 1);
  border-radius: 50%;
  color: #fff;
  text-align: center;
  margin-right: 20rpx;
}

.avatar_group{
  display: flex;
  padding-bottom: 20rpx;
}
.avatar_group .avatar{
  width: 90rpx;
  height: 90rpx;
  position: relative;
}
.avatar_group .avatar .badge2{
  position: absolute;
  bottom: -10rpx;
  left: 0;
  right: 0;
  height: 30rpx;
  text-align: center;
  background-color: #f65236;
  color: #fff;
  font-size: 22rpx;
}

.avatar_df{
  border: 2px dashed #DCDCDC;
}
.btn_box{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #f5f5f5;
}
.page2{
  margin-bottom: 140rpx;
}

.countdown {
  position: relative;
  margin-left: 10rpx;
  font-size: 24rpx;
  top: -3rpx;
}

.countdown text {
  display: inline-block;
  background-color: #333;
  height: 40rpx;
  line-height: 44rpx;
  color: #fff;
  font-size: 24rpx;
  padding: 0 8rpx;
  margin: 0 4rpx;
  vertical-align: middle;
  position: relative;
  top:3rpx;
  font-family: "num";
}

.countdown_box{
  display: flex;
  align-items: center;
}
.zt_img{
  width: 150rpx;
  height: 150rpx;
}
.default{
  background-repeat: no-repeat;
  background-position: left bottom;
  background-size: 100% 4rpx;
  background-image: url(data:image/png;base64,)
}